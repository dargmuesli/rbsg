<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.Pane?>
<?import com.jfoenix.controls.JFXHamburger?>
<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<AnchorPane stylesheets="@../css/dark-design2.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.se1ss19teamb.rbsg.ui.InGameController">
    <AnchorPane fx:id="inGameScreen" stylesheets="@../css/dark-design2.css" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox alignment="CENTER_RIGHT" spacing="10" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="5.0">
            <JFXButton fx:id="btnBack" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false">
                <graphic>
                    <FontAwesomeIconView glyphName="REPLY" />
                </graphic>
            </JFXButton>
            <JFXButton fx:id="btnTicTacToe" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false">
                <graphic>
                    <FontAwesomeIconView boundsType="VISUAL" glyphName="PLUS" size="15" />
                </graphic></JFXButton>
            <JFXButton fx:id="btnLogout" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false">
                <graphic>
                    <FontAwesomeIconView boundsType="VISUAL" glyphName="SIGN_OUT" size="15" />
                </graphic></JFXButton>
            <JFXHamburger fx:id="ham" prefHeight="0.0" prefWidth="47.0" />
        </HBox>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS">
         <children>
                 <HBox alignment="TOP_RIGHT" prefHeight="100" prefWidth="636" />
             <Pane prefWidth="636" prefHeight="395"></Pane>
             <HBox prefHeight="100" prefWidth="100"></HBox>
         </children></VBox>
    </AnchorPane>
</AnchorPane>
