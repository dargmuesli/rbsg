            fx:controller="de.uniks.se1ss19teamb.rbsg.ui.TurnUiController">
    <HBox AnchorPane.leftAnchor="200" AnchorPane.rightAnchor="200" spacing="40">
            <Label fx:id="labelOne" text="Team1" visible="false"></Label>
            <Label fx:id="labelTwo" text="Team2" visible="false"></Label>
            <Label fx:id="labelThree" text="Team3" visible="false"></Label>
            <Label fx:id="labelFour" text="Team4" visible="false"></Label>
        <HBox spacing="10">
        <JFXButton fx:id="phaseBtn" text="Next Phase" onAction="#setOnAction"></JFXButton>
        <Label fx:id="turnLabel" text="test"></Label>
        </HBox>
    </HBox>
</AnchorPane>
